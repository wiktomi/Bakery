@model IEnumerable<Bakery.Models.Shape>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ShapeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ShapeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ShapeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ShapeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ShapeID })
        </td>
    </tr>
}

</table>
